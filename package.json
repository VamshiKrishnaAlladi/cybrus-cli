{
    "name": "cybrus",
    "version": "0.2.0",
    "description": "Cybrus is a CLI app that ends your ultimate quest for the answers.",
    "main": "cybrus.js",
    "author": "Vamshi Krishna Alladi",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/VamshiKrishnaAlladi/cybrus/issues"
    },
    "homepage": "https://github.com/VamshiKrishnaAlladi/cybrus#readme",
    "keywords": [
        "cybrus",
        "BOT",
        "Trick",
        "Prank"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/VamshiKrishnaAlladi/cybrus.git"
    },
    "bin": {
        "cybrus": "./src/cybrus.js"
    },
    "scripts": {
        "test": "jest --coverage --colors && codecov",
        "ci-test": "jest --coverage",
        "ci-cov": "codecov",
        "jest": "jest --colors",
        "codecov": "codecov",
        "precommit": "jest"
    },
    "jest": {
        "coverageDirectory": "./coverage/",
        "collectCoverage": false,
        "testEnvironment": "node",
        "testMatch": [
            "**/*.test.js"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/",
            "/coverage/"
        ],
        "verbose": true
    },
    "engines": {
        "node": "7.5.0",
        "npm": "4.1.2"
    },
    "dependencies": {
        "bluebird": "3.5.0",
        "chalk": "2.0.1",
        "commander": "2.11.0"
    },
    "devDependencies": {
        "codecov": "^2.1.0",
        "grunt": "^1.0.1",
        "grunt-contrib-watch": "^1.0.0",
        "grunt-eslint": "^20.0.0",
        "grunt-shell": "^2.1.0",
        "husky": "^0.14.1",
        "jest-cli": "^20.0.4",
        "load-grunt-tasks": "^3.5.2"
    }
}
